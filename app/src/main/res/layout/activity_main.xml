<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="horizontal"
    tools:context="zxwl.com.texiao.MainActivity">

    <zxwl.com.texiao.CircleMenuLayout
        android:id="@+id/id_menulayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/zzsz6"
        android:padding="100dp">

        <RelativeLayout
            android:id="@id/id_circle_menu_item_center"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="104.0dip"
                android:layout_height="104.0dip"
                android:layout_centerInParent="true"
                android:background="@mipmap/ic_launcher"

                />

            <ImageView
                android:layout_width="116.0dip"
                android:layout_height="116.0dip"
                android:background="@drawable/zzsz1"
                android:layout_centerInParent="true"
                />
        </RelativeLayout>
    </zxwl.com.texiao.CircleMenuLayout>


</LinearLayout>
